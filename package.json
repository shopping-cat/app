{
  "name": "app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "adb devices && react-native run-android",
    "ios": "react-native run-ios",
    "android:release": "adb devices && react-native run-android --variant=release",
    "ios:release": "react-native run-ios --variant=release",
    "android:assembleRelease": "cd android && ./gradlew assembleRelease",
    "android:bundleRelease": "cd android && ./gradlew bundleRelease",
    "codepush:android": "appcenter codepush release-react -a coderhyun/react-graphql-android -d Staging",
    "codepush:ios": "appcenter codepush release-react -a coderhyun/react-graphql -d Staging",
    "gradlew:clean": "cd android && ./gradlew clean && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@apollo/client": "^3.2.4",
    "@invertase/react-native-apple-authentication": "^2.1.0",
    "@react-native-community/async-storage": "^1.12.0",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-native-firebase/app": "^10.1.0",
    "@react-native-firebase/auth": "^10.1.1",
    "@react-native-seoul/kakao-login": "^2.10.1",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "appcenter": "3.1.2",
    "appcenter-analytics": "3.1.2",
    "appcenter-crashes": "3.1.2",
    "graphql": "^15.3.0",
    "react": "16.13.1",
    "react-native": "0.63.2",
    "react-native-autoheight-webview": "^1.5.7",
    "react-native-code-push": "^6.3.0",
    "react-native-daum-postcode": "^1.0.9",
    "react-native-device-info": "^7.3.1",
    "react-native-fbsdk": "^2.0.0",
    "react-native-flipper": "^0.62.0",
    "react-native-flipper-apollo-devtools": "0.0.2",
    "react-native-gesture-handler": "^1.8.0",
    "react-native-hyperlink": "0.0.19",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-modal": "^11.5.6",
    "react-native-reanimated": "^1.13.0",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.10.1",
    "react-native-status-bar-height": "^2.6.0",
    "react-native-svg": "^12.1.0",
    "react-native-tab-view": "^2.15.2",
    "react-native-vector-icons": "^7.1.0",
    "react-native-webview": "^11.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/react-native": "^0.63.2",
    "@types/react-native-fbsdk": "^1.1.0",
    "@types/react-native-vector-icons": "^6.4.5",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "babel-jest": "^25.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
